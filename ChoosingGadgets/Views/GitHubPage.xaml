<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ChoosingGadgets.Views.GitHubPage"
             xmlns:resources="clr-namespace:ChoosingGadgets.Resources"
             Title="{x:Static resources:Localization.MyGitHub}"
             Style="{StaticResource PageStyle}">
    <ContentPage.Content>
        <StackLayout>
            <WebView Source="https://github.com/MaxGog" VerticalOptions="FillAndExpand"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>